+				 (RSb[ (unsigned char) (Y0 >> 16) ] << 16) ^
+				 (RSb[ (unsigned char) (Y3 >>	8) ] <<  8) ^
+				 (RSb[ (unsigned char) (Y2 	 ) ]	  );
+
+	X2 = RK[2] ^ (RSb[ (unsigned char) (Y2 >> 24) ] << 24) ^
+				 (RSb[ (unsigned char) (Y1 >> 16) ] << 16) ^
+				 (RSb[ (unsigned char) (Y0 >>	8) ] <<  8) ^
+				 (RSb[ (unsigned char) (Y3 	 ) ]	  );
+
