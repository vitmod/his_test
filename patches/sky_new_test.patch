+//		MYCONST_TRACE("rcval: %d\n", rcval);
+		if (rcval == 13) singly = 1;
+		if (rcval != 13 && rcval != 21) {
+			rcval = sscanf(token, "%4x:%6x:%4x:%4x:%4x:%4x:%2x %2x %2x %2x %2x %2x %2x %2x %2x %2x %2x %2x %2x %2x %2x %2x",
