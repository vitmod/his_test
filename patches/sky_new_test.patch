+void DES_set_odd_parity(DES_cblock *key)
+{
+	unsigned int i;
+
+	for (i=0; i<DES_KEY_SZ; i++)
+		(*key)[i]=odd_parity[(*key)[i]];
+}
+
+int DES_check_key_parity(const_DES_cblock *key)
+{
+	unsigned int i;
+
+	for (i=0; i<DES_KEY_SZ; i++)
+	{
+		if ((*key)[i] != odd_parity[(*key)[i]])
+			return(0);
+	}
+	return(1);
+}
