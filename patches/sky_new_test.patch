+//		if ((HI_SUCCESS != hiReturn) || (u32HandleNum == 0)) continue;
+		for (f = 0; f < MAX_HIFILTERS; f++)
+		{
+		  	ChFilter = &(g_HIFilters[f]);
+			if ( ChFilter->uFiltype != ChTYPE) continue;
