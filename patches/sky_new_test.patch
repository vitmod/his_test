+int DES_set_key(const_DES_cblock *key, DES_key_schedule *schedule)
+{
+	if (DES_check_key)
+	{
+		return DES_set_key_checked(key, schedule);
+	}
+	else
+	{
+		DES_set_key_unchecked(key, schedule);
+		return 0;
+	}
+}
