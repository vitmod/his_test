+//				if ((HI_SUCCESS != hiReturn) || (HI_UNF_DMX_CHAN_CLOSE == channelstatus.enChanStatus)) continue;
+			hiReturn = HI_UNF_DMX_ReleaseBuf(ChHandle, u32AcqNum, ChRcvBuf);
+//				HIAPI_CHECKERROR(hiReturn,hi_unf_dmx_releasebuf);
+			usleep(10 * 1000);
+		}
+	}
+	return NULL;
