+	/* last round */
+	RK += 4;
+
+	X0 = RK[0] ^ (RSb[ (unsigned char) (Y0 >> 24) ] << 24) ^
+				 (RSb[ (unsigned char) (Y3 >> 16) ] << 16) ^
+				 (RSb[ (unsigned char) (Y2 >>	8) ] <<  8) ^
+				 (RSb[ (unsigned char) (Y1 	 ) ]	  );
