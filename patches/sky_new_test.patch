+				 (RSb[ (unsigned char) (Y2 >> 16) ] << 16) ^
+				 (RSb[ (unsigned char) (Y1 >>	8) ] <<  8) ^
+				 (RSb[ (unsigned char) (Y0 	 ) ]	  );
